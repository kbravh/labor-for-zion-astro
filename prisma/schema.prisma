generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Analytics {
  id          String  @id @default(cuid())
  slug        String
  locale      String
  utm_source  String?
  utm_medium  String?
  utm_campaign String?
  utm_content String?
  utm_term    String?
  timestamp   DateTime @default(now())

  @@index([slug, locale])
}
